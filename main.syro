@swap(i32*: x, i32*: y) -> void {
    i32: temp = *x;
    *x = *y;
    *y = temp;
}

@increment(i32*: value) -> void {
    *value = *value + 1;
}

@main() -> i32 {
    i32: a = 10;
    i32: b = 20;

    print a;
    print b;

    swap(&a, &b);

    print a;
    print b;

    increment(&a);
    increment(&b);

    print a;
    print b;

    return 0;
}
